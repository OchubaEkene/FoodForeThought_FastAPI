{
  "builds": [
    {
      "src": "api/main.py",
      "use": "@vercel/python"
    },
    {
      "src": "static/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/main.py"
    }
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/", "dest": "/api" }
  ]
}
